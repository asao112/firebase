  return {\n      username: '',\n      email: '',\n      password: ''\n    };\n  },\nmethods: {\n    register() {\n      firebase\n        .auth()\n        .createUserWithEmailAndPassword(this.email, this.password)\n        .then(result => {\n          result.user.updateProfile({\n            displayName: this.username\n           })\n      }).catch(error => {\n          alert(error.message);\n        });\n    }\n  }\n};\n</script>\n"]}]}